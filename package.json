{
  "name": "electron-vue-example",
  "author": "shaoyongkai",
  "description": "electron vue cli3",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
